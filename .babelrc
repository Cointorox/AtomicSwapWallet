{
  "presets": [ "react",  ["env", {
    "targets": {
     "browsers": [ ">0.25%", "not ie 11", "not op_mini all"]
    }
  }], "stage-0" ],
  "env": {
   "production": {
      "presets": ["minify"]
    }
  },
  "plugins": [
    [ "transform-object-rest-spread", { "useBuiltIns": true } ],
    "transform-es2015-destructuring",
    "transform-object-rest-spread",
    "transform-runtime",
    "transform-decorators-legacy",
    "transform-class-properties"
  ]
}
