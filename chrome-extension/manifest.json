{
  "name": "Atomicswapwallet.io",
  "version": "1.3.1",
  "manifest_version": 2,
  "description": "Atomicswapwallet.io Chrome Extension",
  "homepage_url": "https://Atomicswapwallet.io/",
  "icons": {
    "16": "extension/images/icons/icon-16.png",
    "32": "extension/images/icons/icon-32.png",
    "192": "extension/images/icons/icon-196.png"
  },
  "options_page": "application/index.html",
  "browser_action": {
    "default_icon": "extension/images/icons/icon-32.png",
    "default_title": "Atomicswapwallet.io"
  },
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": ["./extension/js/background.js"],
    "persistent": false
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
